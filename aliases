#
#These aliases are symlinked and synced to icloud#

alias ll='ls -lAh'
alias lr='ls -lrt'
alias c='cd'
alias d='cd'
alias doc='cd ~/Documents'
alias dwn='cd ~/Downloads'
alias chalias='vi ~/.aliases; source ~/.aliases'
alias dmesg='sudo dmesg'
alias syncalias='source ~/.aliases'
alias chssh='vi ~/.ssh/config'
alias vssh=' vi ~/.ssh/config'
alias help_scp_hop=' echo "scp -o \"ProxyCommand ssh pcreds@proxy.machine -W %h:%p\" tcreds@target.machine:file ."'

